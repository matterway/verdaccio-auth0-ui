{
  "name": "verdaccio-auth0-ui",
  "version": "4.0.1",
  "keywords": [
    "github",
    "login",
    "npm",
    "oauth",
    "package",
    "private",
    "registry",
    "verdaccio"
  ],
  "homepage": "https://github.com/matterway/verdaccio-auth0-ui/blob/master/README.md",
  "bugs": "https://github.com/matterway/verdaccio-auth0-ui/issues",
  "repository": "github:matterway/verdaccio-auth0-ui",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/server.js",
  "bin": {
    "verdaccio-auth0-ui": "dist/cli.js"
  },
  "scripts": {
    "build": "bash run build",
    "lint": "bash run lint",
    "prepack": "bash run clean && bash run build",
    "start": "verdaccio -c verdaccio.yaml",
    "test": "jest",
    "test:cli": "bash run oauth-cli-test",
    "test:coverage": "jest --collectCoverage && codecov",
    "test:manual": "yarn update && yarn start",
    "test:watch": "jest --watch",
    "test:whoami": "bash run whoami",
    "update": "yarn prepack && bash run link",
    "watch": "nodemon --watch src --ext ts,css --exec \"yarn update\""
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "express": "^4.21.1",
    "global-agent": "^2.1.5",
    "got": "^11.8.5",
    "memory-cache": "^0.2.0",
    "minimist": "^1.2.0",
    "open": "^7.0.2",
    "openid-client": "^6.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@n4bb12/config-tslint": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/got": "^9.4.1",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.123",
    "@types/node": "^13.7.0",
    "@verdaccio/types": "^9.3.0",
    "babel-jest": "^29.7.0",
    "codecov": "^3.6.4",
    "core-js": "^3.6.4",
    "jest": "^29.7.0",
    "lodash": "4.17.21",
    "nodemon": "^2.0.2",
    "release-it": "^17.10.0",
    "rollup": "2.79.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.3.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "verdaccio": "^6.0.1",
    "verdaccio-htpasswd": "^9.3.0"
  },
  "peerDependencies": {
    "braces": ">=3.0.3",
    "cookie": ">=0.7.0",
    "lodash": "4",
    "verdaccio": ">=5",
    "rollup": ">=2.79.2",
    "semver": "5.7.2"
  },
  "resolutions": {
    "rollup": "^2.79.2",
    "braces": "^3.0.3",
    "semver": "^5.7.2",
    "cookie": "^0.7.0"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "$schema": "http://json.schemastore.org/package"
}
